{% extends 'base.html.twig' %}

{% block stylesheets %}
{% endblock %}

{% block javascripts %}
{% endblock %}

{% block body %}

    <h2>Demande de contact du site Rise Agency</h2>
    <table>
        <tbody>
        <tr>
            <td>Nom :</td>
            <td>{{ contact.name }}</td>
        </tr>
        <tr>
            <td>Email :</td>
            <td><a href="mailto:{{ contact.email }}">{{ contact.email }}</a></td>
        </tr>
        <tr>
            <td>Objet :</td>
            <td>{{ contact.subject }}</td>
        </tr>
        <tr>
            <td>Message :</td>
            <td>{{ contact.message }}</td>
        </tr>
        </tbody>
    </table>

{% endblock %}